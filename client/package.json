{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-relay": "^11.0.0",
    "react-scripts": "4.0.3",
    "relay-runtime": "^11.0.0",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
